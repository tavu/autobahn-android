<?xml version="1.0" encoding="utf-8"?>

<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">



<LinearLayout
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <LinearLayout
		android:orientation="vertical"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_marginLeft="20px"
		android:layout_marginTop="20px"
		android:layout_marginRight="20px">
		<!--android:showDividers="beginning|middle|end" -->
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/service"
			android:id="@+id/textView"
			android:singleLine="false"
			android:phoneNumber="false"
			android:password="false" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/service" style="@style/AutobahnPropertyValue"/>

        <TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/description"
			android:id="@+id/textView3" style="@style/AutobahnPropertyName" android:singleLine="false"
                />

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/description" style="@style/AutobahnPropertyValue"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reservationState"
                android:id="@+id/textView2" style="@style/AutobahnPropertyName"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/reservationState" style="@style/AutobahnPropertyValue"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/provisionState"
                android:id="@+id/textView13" style="@style/AutobahnPropertyName"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/provisionState" style="@style/AutobahnPropertyValue"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lifecycleState"
                android:id="@+id/textView15" style="@style/AutobahnPropertyName"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/lifecycleState" style="@style/AutobahnPropertyValue"/>
        <TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/start_time"
			android:id="@+id/textView4" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/startTime" style="@style/AutobahnPropertyValue"/>
		`

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView5"
			android:singleLine="false"
			android:text="@string/end_time" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/endTime" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/start_port"
			android:id="@+id/textView6"
			android:singleLine="false"
			android:phoneNumber="true" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/startPort" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/start_vlan"
			android:id="@+id/textView7"
			android:singleLine="true" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/startVlan" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/end_port"
			android:id="@+id/textView8" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/endPort" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/end_vlan"
			android:id="@+id/textView9"
			android:singleLine="false" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/endVlan" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/capacity"
			android:id="@+id/textView10" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/capacity" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/mtu_size"
			android:id="@+id/textView11" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/mtuSize" style="@style/AutobahnPropertyValue"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/failure_reason"
			android:id="@+id/failureReasonLabel" style="@style/AutobahnPropertyName"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/failureReason" style="@style/AutobahnPropertyValue"/>
	</LinearLayout>
</LinearLayout>
</ScrollView>